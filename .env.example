# Required Security Configuration
ADMIN_DASHBOARD_PASSWORD=yourpassword
SESSION_SECRET=random_secret_key_here
# Generate a secure key: require('crypto').randomBytes(32).toString('hex')
TOKEN_ENCRYPTION_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# Master key for creating sessions via API (not needed for admin dashboard)
MASTER_API_KEY=your_master_api_key_here

# Optional Configuration
PORT=3000
MAX_SESSIONS=10
SESSION_TIMEOUT_HOURS=24
# Default webhook URL (can be overridden per session)
WEBHOOK_URL=https://your-default-webhook.com/events

# Production Environment
NODE_ENV=production
# For maintenance mode (set to 'true' to enable)
MAINTENANCE_MODE=false 